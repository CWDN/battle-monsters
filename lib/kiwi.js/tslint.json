{
  "rules": {
	"class-name": false,
	"curly": false,
	"eofline": false,
	"forin": false,
	"indent": [false, 4],
	"label-position": false,
	"label-undefined": false,
	"max-line-length": [false, 140],
	"no-arg": true,
	"no-bitwise": false,
	"no-console": [true,
		"debug",
		"info",
		"time",
		"timeEnd",
		"trace"
	],
	"no-construct": false,
	"no-debugger": true,
	"no-duplicate-key": false,
	"no-duplicate-variable": false,
	"no-empty": false,
	"no-eval": true,
	"no-string-literal": false,
	"no-trailing-comma": false,
	"no-trailing-whitespace": false,
	"no-unused-variable": false,
	"no-unreachable": true,
	"no-use-before-declare": false,
	"one-line": [false,
		"check-open-brace",
		"check-catch",
		"check-else",
		"check-whitespace"
	],
	"quotemark": [false, "double"],
	"radix": false,
	"semicolon": false,
	"triple-equals": [false, "allow-null-check"],
	"variable-name": false,
	"whitespace": [false,
		"check-branch",
		"check-decl",
		"check-operator",
		"check-separator",
		"check-type"
	]
  }
}
